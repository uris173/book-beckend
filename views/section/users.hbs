<div class="row" bis_skin_checked="1">
  <div class="col-12" bis_skin_checked="1">
    <div class="card" bis_skin_checked="1">
      <div class="card-body" bis_skin_checked="1">

        <h4 class="card-title">Пользователи</h4>
        <p class="card-title-desc">Таблица Пользователей</p>

        <div class="table-responsive" bis_skin_checked="1">
          <table class="table table-editable table-nowrap align-middle table-edits">
            <thead>
              <tr style="cursor: pointer;">
                <th>№</th>
                <th>Имя</th>
                <th>Фамилия</th>
                <th>Email</th>
                <th>Номер телефона</th>
              </tr>
            </thead>
            <tbody>
              {{#each user}}
              <tr data-id="3" style="cursor: pointer;">
                <td data-field="id">{{index}}</td>
                <td data-field="name">{{name}}</td>
                <td data-field="name">{{surname}}</td>
                <td data-field="name">{{email}}</td>
                <td data-field="name">{{number}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        <nav aria-label="..." class="mt-3 d-flex justify-content-end">
          <ul class="pagination">
              <li class="page-item">
                <a class="page-link" href="#" tabindex="-1">Назад</a>
              </li>
              {{#each pages}}
              <li class="page-item {{class}}"><a class="page-link" href="?page={{queryNum}}">{{num}}</a></li>
              {{/each}}
              <li class="page-item">
                <a class="page-link" href="#">Далее</a>
              </li>
          </ul>
        </nav>

      </div>
    </div>
  </div> <!-- end col -->
</div>